# include <bits/stdc++.h>
using namespace std;
int main() {
    
    int a[6];
    int *ptr;
    cout<<"Enter 6 elements:\n";
    for(int i=0;i<6;i++){
        cin>>a[i];
    }
    ptr=a;
    cout<<"The array elements are\n";
    for(int i=0;i<6;i++,ptr++){
        cout<<*ptr<<"\t";
    }
    return 0;
}
